// ============================ INTESTINE ====================================
// ============================ INTESTINE CAGE ====================================
{ "beamType":"|NORMAL","beamLongBound":2.0,"beamShortBound":0.35,
  "beamLimitSpring":25000,"beamLimitDamp":120,"beamSpring":18000,"beamDamp":18,
  "beamDeform":80000,"beamStrength":200000 },
// Local tetra
["oi1","oi2"],["oi2","oi3"],["oi3","oi1"],["oi1","oi4"],["oi2","oi4"],["oi3","oi4"],["oi1","oi3"],["oi2","oi4"],

// ============================ INTESTINE TETHERS ====================================
// ================= INTESTINE: toothpick pins to back abdomen ================
{
  "beamType":"|NORMAL",
  "beamLongBound":1.015, "beamShortBound":0.985,     // almost rigid
  "beamSpring":90000, "beamDamp":140,                 // no bounce
  "beamLimitSpring":220000, "beamLimitDamp":600,
  "beamDeform":12000, "beamStrength":14000,          // snap on real impacts
  "deformLimitExpansion":1.01,
  "beamPrecompression":0.995, "beamPrecompressionRange":1.0,
  "breakGroup":"org_pin_int"                          // independent of lungs
},
// Top cluster pins (keep near upper back of abdomen)
["oi1","dummy1_abdomentrl"],  // back-left, top
["oi2","dummy1_abdomentrr"],  // back-right, top
["oi1","dummy1_abdomentrr"],  // cross keeps it centered
["oi2","dummy1_abdomentrl"],

// Mid cluster pins (best if oi5 exists; else duplicate oi1/oi2)
["oi5","dummy1_abdomenbrl"],  // back-left, mid/lower
["oi5","dummy1_abdomentrl"],  // back-left, upper
["oi5","dummy1_abdomentrr"],  // cross to right
["oi5","dummy1_abdomenbrr"],  // back-right, lower

// Bottom cluster pins (best if oi6 exists)
["oi6","dummy1_abdomenbrl"],
["oi6","dummy1_abdomenbrr"],
["oi6","dummy1_pelvisbr"],    // slight tie into pelvis rear = stops bottom wander
["oi6","dummy1_pelvisbf"],

// Small diagonals from front pair to back to stop “poke-through roll”
["oi3","dummy1_abdomentrl"],  // oi3/oi4 are your front/top pair
["oi4","dummy1_abdomentrr"],

// Gentle vertical guides (not bouncy; short span)
{
  "beamType":"|NORMAL",
  "beamLongBound":1.02, "beamShortBound":0.98,
  "beamSpring":70000, "beamDamp":120,
  "beamLimitSpring":160000, "beamLimitDamp":500,
  "beamDeform":11000, "beamStrength":12000,
  "beamPrecompression":0.995, "beamPrecompressionRange":1.0,
  "breakGroup":"org_pin_int"
},
["oi1","dummy1_thoraxmfl"],   // slight top anchor into thorax center-left
["oi2","dummy1_thoraxmfr"],   // and center-right
["oi6","dummy1_pelvismrr"],   // low-right
["oi6","dummy1_pelvismrl"]    // low-left


