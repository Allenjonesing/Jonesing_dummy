"agenty_dummy_mod_car_strength_setting":{
  "information":{
        "authors":"AgentY",
        "name":"Car Strength Settings",
    }
    "slotType" : "agenty_dummy_mod_car_strength_setting",

  "variables": [
    ["name", "type", "unit", "category", "default", "min", "max", "title", "description"]
    ["$adeformscale", "range", "x", "Driver and Passengers", 1, 1, 100, "Deformation Resistance Scale (Up)",  "Scale up how much resistent the car is to deformaton, higher values = deforms less."               {"stepDis":1,   "subCategory":"Car strength settings"}],
    ["$abreakscale",  "range", "x", "Driver and Passengers", 1, 1, 100, "Breaking Resistance Scale (Up)",     "Scale up how much resistent the car is to breaking into pieces, higher values = more force needed to break."  {"stepDis":1,   "subCategory":"Car strength settings"}],
    ["$adeformscale2","range", "x", "Driver and Passengers", 1, 0.01, 1,"Deformation Resistance Scale (Down)","Scale down how much resistent the car is to deformaton, lower values = deforms more."      {"stepDis":0.01,"subCategory":"Car strength settings"}]
    ["$abreakscale2", "range", "x", "Driver and Passengers", 1, 0.01, 1,"Breaking Resistance Scale (Down)",   "Scale down how much resistent the car is to breaking into pieces, lower values = less force needed to break." {"stepDis":0.01,"subCategory":"Car strength settings"}]
  ],

  "scalebeamDeform": "$=$adeformscale*$adeformscale2",
  "scalebeamStrength": "$=$abreakscale*$abreakscale2",
},


}